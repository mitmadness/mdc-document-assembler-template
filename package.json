{
  "name": "mydc-document-assembler",
  "type": "module",
  "scripts": {
    "codegen": "graphql-codegen --config graphql-codegen.ts",
    "build": "tsc",
    "watch": "tsc --watch --preserveWatchOutput",
    "serve": "nodemon --delay 1 -w .env -w .env.defaults -w dist dist/src/main.js",
    "dev": "concurrently --raw \"npm:tunnel --port 8080\" npm:watch npm:serve",
    "tunnel": "lt",
    "run-server": "node dist/src/main.js",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "^20.0.0",
    "npm": "^10.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typed-document-node": "^5.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@parcel/watcher": "^2.3.0",
    "@types/common-tags": "^1.8.2",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.2",
    "@types/lodash-es": "^4.17.9",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^20.6.3",
    "@types/speakingurl": "^13.0.4",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "preact": "^10.17.1",
    "preact-render-to-string": "^6.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@mitm/common": "^2.1.0",
    "@mitm/config": "^2.3.0",
    "@mitm/intl": "^1.7.0",
    "@mitm/logger": "^1.2.0",
    "@mitm/node-common": "^1.0.1",
    "@mydecocloud/interfaces": "^1.0.0",
    "ajv": "^8.12.0",
    "better-sse": "^0.10.0",
    "common-tags": "^1.8.2",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "graphql": "^16.8.1",
    "localtunnel": "^2.0.2",
    "lodash-es": "^4.17.21",
    "micromatch": "^4.0.5",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^21.3.3",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "speakingurl": "^14.0.1",
    "tslib": "^2.6.2"
  },
  "volta": {
    "node": "20.8.1",
    "npm": "10.1.0"
  }
}
